{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid": "830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "env": {
                "CARLA_ROOT": "carla",
                "ROUTES": "leaderboard/data/evaluation_routes/routes_town05_long.xml",
                "TEAM_AGENT": "leaderboard/team_code/interfuser_agent.py",
                "TEAM_CONFIG": "leaderboard/team_code/interfuser_config.py",
                "CHECKPOINT_ENDPOINT": "results/interfuser_result.json",
                "SCENARIOS": "leaderboard/data/scenarios/town05_all_scenarios.json",
                "CARLA_SERVER": "${CARLA_ROOT}/CarlaUE4.sh",
                "PYTHONPATH": "$PYTHONPATH:${CARLA_ROOT}/PythonAPI:${CARLA_ROOT}/PythonAPI/carla:$CARLA_ROOT/PythonAPI/carla/dist/carla-0.9.10-py3.7-linux-x86_64.egg:leaderboard/team_code:leaderboard:scenario_runner",
                "LEADERBOARD_ROOT": "leaderboard",
                "CHALLENGE_TRACK_CODENAME": "SENSORS",
                "PORT": "2000", // same as the carla server port"
                "TM_PORT": "2500", // port for traffic manager, required when spawning multiple servers/clients"
                "DEBUG_CHALLENGE": "0",
                "REPETITIONS": "1 # multiple evaluation runs",
                "SAVE_PATH": "data/eval # path for saving episodes while evaluating",
                "RESUME": "True"
            },
            "program": "${file}",
            "args": [
                "--scenarios",
                "${SCENARIOS}"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}